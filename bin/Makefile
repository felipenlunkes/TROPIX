		-walk -p "*/old.* */new.*" /etc /usr/etc /bin /lib /usr/bin /usr/lib | rm -N
		-walk -p "*/old.* */new.*" /usr/pub /usr/xwin/lib /usr/xwin/bin | rm -N
		-walk -p "*/garfield */bobo */boob" /src/cmd /src/lib /src/sys | rm -N

rmobj:
		walk -p "*.o" . | rm

cd.col:
		-rm cd.col
		mkfst1 -fr cd.col 45M TROPIXSRC cdrom
		mount cd.col /mnt
		cptree -L /src			/mnt/src
		cptree -s /src/sys/tropix/pc	/mnt/src/sys/tropix/pc
		cptree -s /src/lib		/mnt/src/lib
		cptree -s /src/cmd 		/mnt/src/cmd
		cptree -s /usr/include 		/mnt/usr/include
		df /mnt
		umount /mnt

pento:
		-mount /pen
		-cmptree -sg . /pen$cmd
		-forldiff /pen$cmd
		-rm -f garfield
		-umount /pen

penfrom:
		-mount /pen
		cd /pen$cmd &&		\
		cmptree -sg . $cmd;	\
		forldiff $cmd;		\
		rm -f /pen$cmd/garfield
		-umount /pen

pensave:
		-mount /pen
		-pento $cmd
		-umount /pen

